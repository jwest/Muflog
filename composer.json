{
    "name": "jwest/muflog",
    "version": "0.0.2",
    "type": "composer-installer",
    "description": "Create blog with markdown files and convert to static files, all function without php logic for end user",
    "keywords": ["jwest", "muflog", "blog", "markdown", "static", "slim"],
    "homepage": "https://github.com/jwest/Muflog",
    "repositories": {
        "jwest/muflog": {
            "type" : "vcs",
            "vcs": {
                "url" : "git://github.com/jwest/Muflog.git"
            }
        }
    },
    "license" : "New BSD",
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.2.0",
        "knplabs/gaufrette": "0.2.*@dev",
        "dflydev/markdown": "1.0.*@dev",
        "fusepump/cli.php": "dev-master",
    },
    "authors": [
        {
            "name": "jwest",
            "email": "jwest@jwest.pl"
        }
    ],
    "autoload": {
        "psr-0": {
            "Muflog": "src/"
        }
    },
    "minimum-stability": "dev"
}